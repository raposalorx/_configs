text/html;                      links %s; nametemplate=%s.html
text/html; elinks -dump -eval "set document.codepage.assume = %{charset}" %s; copiousoutput
image/*;						feh --scale-down %s
image/gif;						mpv %s
application/pdf;				zathura %s; nametemplate=%s.pdf
video/*;						mpv %s
video/mp4;						mpv %s
video/x-youtube;						mpv %s

